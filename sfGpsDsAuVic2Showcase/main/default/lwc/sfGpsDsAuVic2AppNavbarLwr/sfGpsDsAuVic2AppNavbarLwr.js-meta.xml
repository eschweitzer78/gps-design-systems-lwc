<?xml version="1.0" encoding="UTF-8" ?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
  <apiVersion>60.0</apiVersion>
  <isExposed>true</isExposed>
  <masterLabel>VIC2 DS LWR App Navbar</masterLabel>
  <targets>
    <target>lightningCommunity__Page</target>
    <target>lightningCommunity__Default</target>
  </targets>
  <targetConfigs>
    <targetConfig targets="lightningCommunity__Default">
      <property
        name="sectionTitle"
        type="String"
        label="Title"
        default="Title"
      />
      <property
        name="sectionColor"
        type="String"
        label="Color"
        default="var(--rpl-clr-primary)"
      />
      <property
        name="logoSrc"
        type="String"
        label="Logo URL - leave empty for default."
      />
      <property
        name="logoAlt"
        type="String"
        label="Logo alternative text - leave empty for default."
      />
    </targetConfig>
  </targetConfigs>
</LightningComponentBundle>
