                        "items": "[\n\t{\n\t\t\"text\": \"Default blueprint\",\n\t\t\"url\": \"#default-blueprint\",\n\t\t\"items\": [\n\t\t\t{\"text\": \"Example\", \"url\": \"#default-blueprint-example\" },\n\t\t\t{\"text\": \"Configuration\",\"url\":\"#default-blueprint-configuration\" }\n\t\t]},\n\n\t{\n\t\t\"text\": \"Reverse blueprint\",\n\t\t\"url\": \"#reverse-blueprint\",\n\t\t\"items\": [\n\t\t\t{\"text\": \"Example\", \"url\": \"#reverse-blueprint-example\" },\n\t\t\t{\"text\": \"Configuration\",\"url\":\"#reverse-blueprint-configuration\" }\n\t\t]},\n\n\t{\n\t\t\"text\": \"Image blueprint\",\n\t\t\"url\": \"#image-blueprint\",\n\t\t\"items\": [\n\t\t\t{\"text\": \"Example\", \"url\": \"#image-blueprint-example\" },\n\t\t\t{\"text\": \"Configuration\",\"url\":\"#image-blueprint-configuration\" }\n\t\t]},\n\n\t{\n\t\t\"text\": \"Introduction blueprint\",\n\t\t\"url\": \"#introduction-blueprint\",\n\t\t\"items\": [\n\t\t\t{\"text\": \"Example\", \"url\": \"#introduction-blueprint-example\" },\n\t\t\t{\"text\": \"Configuration\",\"url\":\"#introduction-blueprint-configuration\" }\n\t\t]},\n\t{\n\t\t\"text\": \"Documentation\",\n\t\t\"url\": \"#documentation\"\n\t}\n]"
