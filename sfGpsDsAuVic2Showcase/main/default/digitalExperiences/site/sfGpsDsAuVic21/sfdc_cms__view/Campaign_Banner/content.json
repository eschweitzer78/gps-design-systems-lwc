                        "items": "[\n\t{\n\t\t\"text\": \"Featured or primary blueprint\",\n\t\t\"url\": \"#primary-blueprint\",\n\t\t\"items\": [\n\t\t\t{\"text\": \"Example\", \"url\": \"#primary-blueprint-example\" },\n\t\t\t{\"text\": \"Configuration\",\"url\":\"#primary-blueprint-configuration\" }\n\t\t]},\n\n\t{\n\t\t\"text\": \"Image inset or secondary blueprint\",\n\t\t\"url\": \"#secondary-blueprint\",\n\t\t\"items\": [\n\t\t\t{\"text\": \"Example\", \"url\": \"#secondary-blueprint-example\" },\n\t\t\t{\"text\": \"Configuration\",\"url\":\"#secondary-blueprint-configuration\" }\n\t\t]},\n\t{\n\t\t\"text\": \"Documentation\",\n\t\t\"url\": \"#documentation\"\n\t}\n]"
