                      "definition": "c:sfGpsDsAuVic2ContentComm",
                      "attributes": {
                        "className": "rpl-u-margin-b-4",
                        "markup": "<div class=\"app-wysiwyg\">\n\n\t<h2 id=\"default-blueprint\">Default blueprint</h2>\n\n\t\n\n\t<h3 id=\"default-blueprint-example\">Example</h3>\n</div>"
                      }
                    },
                    {
                      "id": "52e07e9b-7d97-4572-90f2-b4a243808bda",
                      "type": "component",
                      "definition": "c:sfGpsDsAuVic2DocsExampleComm",
