<?xml version="1.0" encoding="UTF-8" ?>
<FlexiPage xmlns="http://soap.sforce.com/2006/04/metadata">
  <masterLabel>ServiceNotAvailable</masterLabel>
  <type>CommThemeLayoutPage</type>

  <template>
    <name>siteforce:themeLayoutStarter</name>
    <properties>
      <name>fixedPageWidth</name>
      <value>1170</value>
    </properties>
    <properties>
      <name>headerBgColor</name>
      <value>rgba(0, 0, 0, 0)</value>
    </properties>
    <properties>
      <name>isHeaderPinned</name>
      <value>false</value>
    </properties>
    <properties>
      <name>isHeroUnderHeader</name>
      <value>false</value>
    </properties>
    <properties>
      <name>isPageWidthFixed</name>
      <value>true</value>
    </properties>
    <properties>
      <name>showHeader</name>
      <value>true</value>
    </properties>
    <properties>
      <name>showHero</name>
      <value>true</value>
    </properties>
    <properties>
      <name>themeFooter</name>
    </properties>
    <properties>
      <name>themeHeader</name>
    </properties>
    <properties>
      <name>themeHero</name>
    </properties>
  </template>

  <flexiPageRegions>
    <name>themeHeader</name>
    <type>Region</type>

    <appendable>enabled</appendable>
    <prependable>enabled</prependable>
    <replaceable>enabled</replaceable>
  </flexiPageRegions>

  <flexiPageRegions>
    <name>themeFooter</name>
    <type>Region</type>

    <appendable>enabled</appendable>
    <prependable>enabled</prependable>
    <replaceable>enabled</replaceable>
  </flexiPageRegions>
</FlexiPage>
