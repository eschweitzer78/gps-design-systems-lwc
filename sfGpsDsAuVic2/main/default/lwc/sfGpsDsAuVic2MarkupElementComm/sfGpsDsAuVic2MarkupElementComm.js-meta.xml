<?xml version="1.0" encoding="UTF-8" ?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
  <apiVersion>64.0</apiVersion>
  <isExposed>true</isExposed>
  <masterLabel>VIC2 DS Markup Element</masterLabel>
  <targets>
    <target>lightningCommunity__Page</target>
    <target>lightningCommunity__Default</target>
  </targets>
  <targetConfigs>
    <targetConfig targets="lightningCommunity__Default">
      <property
        name="markup"
        type="String"
        label="Markup"
        description="HTML markup - JavaScript is not supported and will be stripped."
        default="&lt;p&gt;Use this HTML Editor to add your own markup.&lt;/p&gt;"
        editor="c/sfGpsDsMarkupCpe"
      />
      <property
        name="className"
        type="String"
        label="Class name"
        description="Additional CSS class to add."
      />
    </targetConfig>
  </targetConfigs>
</LightningComponentBundle>
