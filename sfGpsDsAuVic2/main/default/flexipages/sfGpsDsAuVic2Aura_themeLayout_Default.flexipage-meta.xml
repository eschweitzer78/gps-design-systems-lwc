<?xml version="1.0" encoding="UTF-8" ?>
<FlexiPage xmlns="http://soap.sforce.com/2006/04/metadata">
  <masterLabel>VIC DS v2 Default Layout</masterLabel>
  <type>CommThemeLayoutPage</type>

  <template>
    <name>sfGpsDsAuVic2ThemeLayout</name>
    <properties>
      <name>noWidgetMargin</name>
      <value>true</value>
    </properties>
    <properties>
      <name>altSections</name>
      <value>true</value>
    </properties>
  </template>

  <flexiPageRegions>
    <name>alertContainer</name>
    <type>Region</type>
    <appendable>enabled</appendable>
    <prependable>enabled</prependable>
    <replaceable>enabled</replaceable>
  </flexiPageRegions>

  <flexiPageRegions>
    <name>header</name>
    <type>Region</type>
    <appendable>enabled</appendable>
    <prependable>enabled</prependable>
    <replaceable>enabled</replaceable>

    <itemInstances>
      <componentInstance>
        <componentName>sfGpsDsAuVic2PrimaryNavComm</componentName>
        <identifier>sfGpsDsAuVic2PrimaryNavComm</identifier>
        <componentInstanceProperties>
          <name>mode</name>
          <value>Demo</value>
        </componentInstanceProperties>
      </componentInstance>
    </itemInstances>
  </flexiPageRegions>

  <flexiPageRegions>
    <name>footer</name>
    <type>Region</type>

    <appendable>enabled</appendable>
    <prependable>enabled</prependable>
    <replaceable>enabled</replaceable>

    <itemInstances>
      <componentInstance>
        <componentName>sfGpsDsAuVic2FooterComm</componentName>
        <identifier>sfGpsDsAuVic2FooterComm</identifier>
        <componentInstanceProperties>
          <name>mode</name>
          <value>Demo</value>
        </componentInstanceProperties>
        <componentInstanceProperties>
          <name>linksMode</name>
          <value>Demo</value>
        </componentInstanceProperties>
      </componentInstance>
    </itemInstances>
  </flexiPageRegions>

  <flexiPageRegions>
    <name>themeHeader</name>
    <type>Region</type>
    <appendable>enabled</appendable>
    <prependable>enabled</prependable>
    <replaceable>enabled</replaceable>
  </flexiPageRegions>

  <flexiPageRegions>
    <name>sidebar</name>
    <type>Region</type>
    <appendable>enabled</appendable>
    <prependable>enabled</prependable>
    <replaceable>enabled</replaceable>
  </flexiPageRegions>
</FlexiPage>
